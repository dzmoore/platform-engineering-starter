{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.13.7/.schema/devbox.schema.json",
  "packages": [
    "colima@latest",
    "docker@latest",
    "kind@latest",
    "kubectl@latest",
    "kubernetes-helm@latest",
    "just@latest",
    "kcl@latest",
    "kyverno-chainsaw@latest",
    "crossplane-cli@latest",
    "yq-go@latest",
    "google-cloud-sdk@latest"
  ],
  "shell": {
    "init_hook": [
      "if [[ \"$SHELL\" == *\"bash\"* ]]; then source \"$DEVBOX_PROJECT_ROOT/devbox-init-bash.sh\"; fi",
      "if [[ \"$SHELL\" == *\"zsh\"* ]]; then source \"$DEVBOX_PROJECT_ROOT/devbox-init-zsh.sh\"; fi"
    ],
    "scripts": { }
  }
}
