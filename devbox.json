{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.13.7/.schema/devbox.schema.json",
  "packages": [
    "colima@latest",
    "docker@latest",
    "kind@latest",
    "kubectl@latest",
    "kubernetes-helm@latest",
    "just@latest",
    "kcl@latest",
    "kyverno-chainsaw@latest",
    "crossplane-cli@latest",
    "yq-go@latest",
    "google-cloud-sdk@latest",
    "krew@latest",
    "fzf@latest"
  ],
  "shell": {
    "init_hook": [
      "if [ \"${-#*i}\" != \"$-\" ]; then",
      "case \"$SHELL\" in *bash*) source \"$DEVBOX_PROJECT_ROOT/devbox-init-bash.sh\";; esac",
      "case \"$SHELL\" in *zsh*) source \"$DEVBOX_PROJECT_ROOT/devbox-init-zsh.sh\";; esac",
      "fi",
      "export PATH=\"${KREW_ROOT:-$HOME/.krew}/bin:$PATH\"",
      "krew update 1> /dev/null 2> /dev/null && krew install ns ctx 1> /dev/null 2> /dev/null"
    ],
    "scripts": {}
  },
  "env": {}
}
